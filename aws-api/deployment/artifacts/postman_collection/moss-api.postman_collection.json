                {
                  "name": "IndexArtefact",
                  "event": [
                    {
                      "listen": "test",
                      "script": {
                        "exec": [
                          "",
                          "console.log('ARTEFACTID --->>>> ' + pm.environment.get(\"artefactId\"));",
                          "var currentURL = pm.request.url.toString();",
                          "",
                          "// Test positive scenarios",
                          "const reqWithValidMirisDocId = {mirisDocId:'12345'};",
                          "var invalidArtefactId = pm.environment.get(\"artefactId\");",
                          "var updatedUrl = currentURL.replace(/\\/([^\\/]+)$/, '/' + invalidArtefactId);",
                          "console.log('invalidArtefactId URL : ' + updatedUrl);",
                          "",
                          "pm.sendRequest(",
                          "    {",
                          "        url : updatedUrl,",
                          "        method: 'PUT',",
                          "        header:  {",
                          "            'Content-Type' : 'application/json'",
                          "        },",
                          "        body :{",
                          "            mode :'raw',",
                          "            raw: JSON.stringify(reqWithValidMirisDocId)",
                          "",
                          "        }",
                          "    }, function (err, response) {",
                          "        if(err){",
                          "            console.log(err);",
                          "        }else{",
                          "            pm.test(\"Successfully index an artefact with valid data\", function () {",
                          "                console.log(response.json())",
                          "                    pm.expect(response).not.to.be.empty;",
                          "                    pm.expect(response.code).to.be.equals(200)",
                          "                    pm.expect(response.json().message).to.be.equals(\"Successfully indexed the Artefact\")",
                          "            });",
                          "",
                          "        }",
                          "    }",
                          ");",
                          "",
                          "pm.sendRequest(",
                          "    {",
                          "        url : currentURL,",
                          "        method: 'PUT',",
                          "        header:  {",
                          "            'Content-Type' : 'application/json'",
                          "        },",
                          "        body :{",
                          "            mode :'raw',",
                          "        }",
                          "    }, function (err, response) {",
                          "        if(err){",
                          "            console.log(err);",
                          "        }else{",
                          "            pm.test(\"Status code is 400 when body is empty\", function () {",
                          "                    pm.expect(response).not.to.be.empty;",
                          "                    pm.expect(response.code).to.be.equals(400)",
                          "                    pm.expect(response.json().message).to.be.equals(\"Empty request body\")",
                          "            });",
                          "",
                          "        }",
                          "    }",
                          ");",
                          "",
                          "",
                          "",
                          "const reqWithoutMirisDocId = {mirisdocId:'test'};",
                          "",
                          "pm.sendRequest(",
                          "    {",
                          "        url : currentURL,",
                          "        method: 'PUT',",
                          "        header:  {",
                          "            'Content-Type' : 'application/json'",
                          "        },",
                          "        body :{",
                          "            mode :'raw',",
                          "            raw: JSON.stringify(reqWithoutMirisDocId)",
                          "        }",
                          "    }, function (err, response) {",
                          "        if(err){",
                          "            console.log(err);",
                          "        }else{",
                          "            pm.test(\"Status code is 400 when mirisDocId is not passed\", function () {",
                          "                    pm.expect(response).not.to.be.empty;",
                          "                    pm.expect(response.code).to.equal(400);",
                          "",
                          "                    pm.expect(response.json().message).to.be.equals(\"Failed to parse mirisDocId from request body\")",
                          "            });",
                          "",
                          "        }",
                          "    }",
                          ");",
                          "",
                          "const reqWithUnknownArtefactId = {mirisDocId:'46578'};",
                          "// Negative scenario - constructing URL for negative scenario",
                          "var invalidArtefactId = '1';",
                          "var updatedUrl = currentURL.replace(/\\/([^\\/]+)$/, '/' + invalidArtefactId);",
                          "console.log('invalidArtefactId URL : ' + updatedUrl);",
                          "pm.sendRequest(",
                          "    {",
                          "        url : updatedUrl,",
                          "        method: 'PUT',",
                          "        header:  {",
                          "            'Content-Type' : 'application/json'",
                          "        },",
                          "        body :{",
                          "            mode :'raw',",
                          "            raw: JSON.stringify(reqWithUnknownArtefactId)",
                          "        }",
                          "    }, function (err, response) {",
                          "        if(err){",
                          "            console.log(err);",
                          "        }else{",
                          "            pm.test(\"Status code is 404 when artefact is not found\", function () {",
                          "                    let respJson = response.json();",
                          "                    pm.expect(respJson).not.to.be.empty;",
                          "                    pm.expect(response.code).to.equal(404);",
                          "                    pm.expect(respJson.message).to.be.equals(\"The artefact is not found with id 1\")",
                          "            });",
                          "",
                          "        }",
                          "    }",
                          ");",
