                      "                type :'new',",
                      "                artefactName : 'application',",
                      "                artefactClassType : 'DOCUMENT',",
                      "                batchSequence : '2002',",
                      "                creationDate : '20221128',",
                      "                requestType :  'application.pdf',",
                      "                filename :  'test',",
                      "                path :  'pdf',",
                      "                contentType :  'pdf',",
                      "                user :  'user1'",
                      "",
                      "            }];",
                      "pm.sendRequest({",
                      "    url : batchUploadURL,",
                      "    method: 'POST',",
                      "    header:  {",
                      "        'Content-Type' : 'application/json'",
                      "    },",
                      "    body :{",
